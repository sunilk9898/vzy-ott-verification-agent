{
  "name": "vzy-ott-verification-agent",
  "version": "1.0.0",
  "description": "Autonomous AI Agent System for OTT Website Security, Performance & Code Quality Analysis",
  "main": "src/orchestrator/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/orchestrator/index.js",
    "start:dev": "ts-node src/orchestrator/index.ts",
    "scan": "ts-node src/cli.ts scan",
    "scan:security": "ts-node src/cli.ts scan --agent security",
    "scan:performance": "ts-node src/cli.ts scan --agent performance",
    "scan:code": "ts-node src/cli.ts scan --agent code",
    "scan:full": "ts-node src/cli.ts scan --agent all",
    "dashboard": "ts-node src/dashboard/server.ts",
    "scheduler": "ts-node src/scheduler/cron.ts",
    "test": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/ --ext .ts",
    "docker:build": "docker build -t vzy-agent .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.0.0",
    "chrome-launcher": "^1.1.0",
    "commander": "^12.0.0",
    "cron": "^3.1.0",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.0",
    "jira-client": "^8.2.0",
    "jsonwebtoken": "^9.0.3",
    "lighthouse": "^12.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.0",
    "openai": "^4.60.0",
    "pg": "^8.13.0",
    "puppeteer": "^23.0.0",
    "retire": "^5.0.0",
    "socket.io": "^4.7.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
